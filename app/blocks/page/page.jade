include ../main-header/main-header
include ../content-section/content-section
include ../blog-section/blog-section


mixin page()
	+b('body').page&attributes(attributes)
		// Header section
		+main-header
		// Content section
		main
			+content-section.section
				+blog-section(getData('posts')).container
		+scripts(['app.min.js'])
